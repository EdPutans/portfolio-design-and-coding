{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "react-app"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "unicorn",
    "prettier",
    "react"
  ],
  "rules": {
    "prettier/prettier": "error",
    "global-require": 0,
    "no-use-before-define": 0,
    "react/destructuring-assignment": 0,
    "react/jsx-filename-extension": [2, {"extensions": [".js"]}],
    "import/no-cycle": 2,
    "unicorn/import-index": 2,
    "no-unused-vars": 2
  },
  "env": {
    "jest": true,
    "browser": true,
    "node": true
  }
}
